<!DOCTYPE html>
<html>
<head>
  <title><%= t 'site_name' %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "header" %>
  <div class="container-fluid">
    <div class="row">
      <%= yield %>
    </div>
  </div>
  <%= render "footer" %>
</body>
</html>
